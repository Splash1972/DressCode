<h1>Dashboard</h1>
<form id="attire-form">
    <input type="text" id="weather" name="weather" placeholder="Weather">
    <input type="text" id="event" name="event" placeholder="Event">
    <input type="text" id="suggestion" name="suggestion" placeholder="Suggestion">
    <button type="submit">Add Attire</button>
</form>
<div>
    {{#each attires}}
        <div>
            <p>Weather: {{this.weather}}</p>
            <p>Event: {{this.event}}</p>
            <p>Suggestion: {{this.suggestion}}</p>
        </div>
    {{/each}}
</div>
<script src="/js/dashboard.js"></script>
